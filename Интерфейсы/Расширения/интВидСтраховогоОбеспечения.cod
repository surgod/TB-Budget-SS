extends Бюджет_ЗПиДД.Справочники.интВидСтраховогоОбеспечения ;

import СИС2 classes ФункцииРТТИ,ФункцииДокумента;

inclass public

  proc ИнициализироватьИнтерфейс synonym InitInterface;
    var локМетод : MethodInfo;
    var локПолнИмя : string;
      inherited InitInterface;
      локПолнИмя = "Бюджет_ЗПиДД.Справочники.интВидСтраховогоОбеспечения.УдалитьНастройкуВидаСОПередУдалением";
      локМетод = ПолучитьИнфоМетодаПоПолномуИмениМетода(локПолнИмя);
      СобытиеПередУдалением.AddMethod(локМетод);
  end;


func УдалитьНастройкуВидаСОПередУдалением(локПарам[] : variant) : logical;
  var Q: Query;
  var локВСО : Бюджет_ЗПиДД.Справочники.ВидыСтраховогоОбеспечения;
    локВСО = локПарам[2];
    if локВСО = nil then
      return false;
    end;
    Result = nil;
    Q = Query.Create([Настройки.Справочники.ВидыСтраховогоОбеспечения]);
    Q.Filter = "ВидСтраховогоОбеспечения="+Str(локВСО);
    Q.Select;
    СИС2.ФункцииДокумента.DeleteAllRecords(Q);
end;



end