class inherited Данные.редБазоваяЗаявкаНаОплатуПособия "Заявка на пособие по врем.нетруд, беременности", editor Данные.ЗаявкаНаНазначениеПособия;

import classes Константы,ФункцииПримененияКостант;--,ФункцииДанных;


inobject private

--Связанные записи

var БольничныйЛист : Документы.Кадры.БольничныйЛистСС;
var ИнтерфейсБЛ : Документы.Кадры.интБольничныйЛистСС;


--------------------------------------------------------------------------------
--{ Обработчики событий бланка

--}

--{ Вспомогательные методы

proc НайтиСвязанныеЗаписи;
  inherited НайтиСвязанныеЗаписи;
  ИнтерфейсБЛ = nil;
  if (ТипДокОснования = ТипГрДок_БЛ) :
    БольничныйЛист = Документы.Кадры.интБольничныйЛистСС.БольничныйЛистПоЗаявке(Self.Record);
    if (БольничныйЛист <> nil) :
      ИнтерфейсБЛ = Документы.Кадры.интБольничныйЛистСС.ОткрытьПоЗаписи(БольничныйЛист);
    fi;
  fi;
end;


--}
end